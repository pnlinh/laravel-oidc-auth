{
    "name": "pnlinh/laravel-oidc-auth",
    "description": "OpenID Connect authentication for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelOIDCAuth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "pnlinh",
            "email": "pnlinh1207@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^7.3|^8.0",
        "steverhoades/oauth2-openid-connect-client": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.53"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelOIDCAuth\\OIDCAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ]
    }
}
